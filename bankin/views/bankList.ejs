<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>List Bank</title>

        <link rel="stylesheet" href="/css/bootstrap.min.css" media="screen" charset="utf-8">
        <link rel="stylesheet" href="/css/style.css" media="screen" charset="utf-8">
        <!-- <link rel="stylesheet" href="/css/iconfont.css" media="screen" charset="utf-8"> -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css">
        <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700&subset=latin,vietnamese' rel='stylesheet' type='text/css'>

        <!-- <script type="text/javascript" src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js'></script> -->

    </head>

    <body>

        <div class="wrapper-bankDetail" style="padding: 15px;">
            <% result.forEach(function(bank, index){ %>
            <div class="bank row hvr-shrink">
                <a href="/bank/<%= bank._id %>">
                    <div class="col-xs-4 logo-listbank">
                        <% if(bank.provider){ %>
                            <img src="images/<%= bank.provider.icon %>" alt="logo"/>
                        <% } else { %>
                            <img src="images/bank_default.png" alt="logo"/>
                        <% } %>
                    </div>
                    <div class="col-xs-8 name-listbank">
                        <div><%= bank.bankId.name %></div>
                        <div>Lãi suất</div>
                        <div class="row showIR">
                            <div class="col-xs-4 text-center">
                                <div class="plan"><i class="fa fa-car fa-fw" aria-hidden="true"></i>Mua xe</div>
                                <div><%= bank.car.interestRate %><small>%</small></div>
                            </div>
                            <div class="col-xs-4 text-center">
                                <div class="plan"><i class="fa fa-home fa-fw" aria-hidden="true"></i>Mua nhà</div>
                                <div><%= bank.house.interestRate %><small>%</small></div>
                            </div>
                            <div class="col-xs-4 text-center">
                                <div class="plan"><i class="fa fa-male fa-fw" aria-hidden="true"></i>Cá nhân</div>
                                <div><%= bank.personal.interestRate %><small>%</small></div>
                            </div>
                        </div>
                    </div>
                </a>
            </div><!-- /bank-->
            <% })  %>
        </div><!-- /wrapper-bankDetail-->
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
        ​
          ga('create', 'UA-39420218-15', 'auto');
          ga('send', 'pageview');
        ​
        </script>
    </body>
</html>
