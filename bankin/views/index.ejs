<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Bank Items</title>

        <link rel="stylesheet" href="/css/bootstrap.min.css" media="screen" charset="utf-8">
        <link rel="stylesheet" href="/css/style.css" media="screen" charset="utf-8">
        <link rel="stylesheet" href="/css/iconfont.css" media="screen" charset="utf-8">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css">
        <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700&subset=latin,vietnamese' rel='stylesheet' type='text/css'>

        <script type="text/javascript" src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js'></script>
        <!-- <script type="text/javascript" src='/js/action.js'></script> -->

    </head>

    <body>
        <script type="text/javascript">
            // window.setTimeout(function(){
            //     window.location.href = '/loan';
            // }, 1000);
            window.location.href = '/loan';
        </script>

        <div class="wrapper">
            <div class="headerTitle row">
                <!-- <div class="lineColor"></div> -->
                <div class="col-xs-1">
                    <span class="icon icon-home icon-for-header-title"></span>
                    <!-- <i class="fa fa-building-o fa-2x" aria-hidden="true" style="color: #2DB04A"></i> -->
                </div>
                <div class="col-xs-5">
                    <div class="header-title">
                        <div>BANK</div>
                        <div>ITEMS</div>
                    </div>
                </div>
            </div><!-- /headerTitle-->

            <div class="content">
                <div class="row">

                    <div class="col-xs-5 col-xs-offset-1 hvr-shrink" style="padding-right: 5px">
                        <div class="loan">
                            <a href="/loan">
                                <div class="imageItem"></div>
                                <div class="textItem">
                                    Vay Vốn
                                </div>
                            </a>
                        </div><!-- /loan-->
                    </div>

                    <div class="col-xs-5" style="padding-left: 5px">
                        <div class="saving">
                            <!-- <a href=""> -->
                                <div class="imageItem"></div>
                                <div class="textItem">
                                    Tiết Kiệm
                                </div>
                            <!-- </a> -->
                        </div><!-- /saving-->
                    </div>

                    <div class="col-xs-5 col-xs-offset-1" style="padding-right: 5px">
                        <div class="insurant">
                            <!-- <a href=""> -->
                                <div class="imageItem">
                                </div>
                                <div class="textItem">
                                    Bảo Hiểm
                                </div>
                            <!-- </a> -->
                        </div><!-- /insurant-->
                    </div>

                    <div class="col-xs-5" style="padding-left: 5px">
                        <div class="creditCard">
                            <!-- <a href=""> -->
                                <div class="imageItem">
                                </div>
                                <div class="textItem">
                                    Thẻ Tín Dụng
                                </div>
                            <!-- </a> -->
                        </div><!-- /creditCard-->
                    </div>

                </div><!-- /row-->
            </div><!-- /content-->
            <div class='error' style='display:none'>Coming Soon</div>
            <script type="text/javascript">
                $('.creditCard').click(function(){
                    $('.error').fadeIn(200).delay(1000).fadeOut(200);
                });
                $('.insurant').click(function(){
                    $('.error').fadeIn(200).delay(1000).fadeOut(200);
                });
                $('.saving').click(function(){
                    $('.error').fadeIn(200).delay(1000).fadeOut(200);
                });
            </script>
        </div><!-- /wrapper-->
    </body>
</html>
